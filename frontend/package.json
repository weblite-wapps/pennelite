{
  "name": "pennelite",
  "version": "1.0.0",
  "scripts": {
    "dev": "node ../../../tools/buildlite/src/index.js default dev",
    "build": "../../../tools/buildlite/src/index.js default build"
  },
  "buildlite": {
    "default": {
      "entry": "./src/setup/index.jsx",
      "outputPath": "dist",
      "outputName": "[name].[hash].js",
      "style": "style.[hash].css",
      "html": {
        "templatePath": "./public/index.html"
      },
      "copy": [
        {
          "from": "./public",
          "to": "."
        }
      ]
    },
    "dev": {
      "react": "dev",
      "devServer": 8000
    }
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "history": "^4.7.2",
    "node-sass": "^4.11.0",
    "ramda": "^0.25.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.3.3",
    "superagent": "^4.1.0"
  }
}
