{
  "name": "frontend",
  "version": "0.1.0",
  "scripts": {
    "dev": "../../../tools/buildlite/src/index.js default dev"
  },
  "buildlite": {
    "default": {
      "entry": "./src/setup/index.jsx",
      "outputPath": "dist",
      "outputName": "[name].[hash].js",
      "style": "style.[hash].css",
      "html": {
        "templatePath": "./public/index.html"
      },
      "copy": [
        {
          "from": "./public",
          "to": "."
        }
      ]
    },
    "dev": {
      "react": "dev",
      "devServer": 8000
    }
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-scripts": "2.1.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
