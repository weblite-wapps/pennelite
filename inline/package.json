{
  "name": "inline",
  "version": "1.0.0",
  "scripts": {
    "dev": "buildlite default dev",
    "build": "buildlite default build"
  },
  "buildlite": {
    "default": {
      "entry": "./src/setup/index.jsx",
      "outputPath": "dist",
      "outputName": "[name].[hash].js",
      "style": "style.[hash].css",
      "html": {
        "templatePath": "./public/index.html"
      },
      "copy": [
        {
          "from": "./public",
          "to": "."
        }
      ]
    },
    "dev": {
      "react": "dev",
      "devServer": 8001
    }
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "react-hot-loader": "^4.6.3",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-observable": "^1.0.0",
    "ramda": "^0.25.0",
    "redux-actions": "^2.6.4",
    "rxjs": "^6.3.3",
    "superagent": "^4.1.0"
  }
}
